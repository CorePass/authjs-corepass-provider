{
    "name": "authjs-corepass-provider",
    "version": "0.2.3",
    "description": "CorePass provider + pending registration/enrichment helpers for Auth.js (@auth/core)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/CorePass/authjs-corepass-provider.git"
    },
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./provider": {
            "types": "./dist/provider.d.ts",
            "import": "./dist/provider.js"
        }
    },
    "files": [
        "dist",
        "db"
    ],
    "keywords": [
        "auth",
        "authjs",
        "core",
        "passkey",
        "corepass",
        "webauthn",
        "passkey-provider"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "@noble/curves": "^2.0.1",
        "blockchain-wallet-validator": "^1.2.1",
        "cbor-x": "^1.6.0"
    },
    "peerDependencies": {
        "@auth/core": ">=0.34.3"
    },
    "overrides": {
        "@simplewebauthn/browser": "^13.0.0",
        "@simplewebauthn/server": "^13.0.0"
    },
    "devDependencies": {
		"@auth/core": ">=0.34.3",
		"@types/node": "^25.2.3",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	}
}

